{
    "name": "romanzipp/laravel-turnstile",
    "description": "Cloudflare Turnstile package for Laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\Turnstile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\Turnstile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\Turnstile\\Providers\\TurnstileServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
